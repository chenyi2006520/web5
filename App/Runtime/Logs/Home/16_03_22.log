SQL: INSERT INTO `b_article` (`k_pinyinName`,`k_md5name`,`k_sort`,`a_title`,`a_author`,`a_time`,`a_content`,`a_addTime`) VALUES ('songzhongjiliguangzhu','a18bd63e520c03c6b712a6755bb622b9','8','宋仲基崔始源权志龙李光洙 韩星兄妹画风相差大? ','人民网','2016-03-20 13:28:00','&lt;p  class=&quot;text-center&quot;&gt;&lt;img src=&quot;http://p34.qhimg.com/t011b8ee6dc6a5001dd.jpg&quot;&gt;&lt;/p&gt;&lt;p&gt;&lt;a  href=&quot;http://songzhongji.ba.cc&quot;  class=&quot;qkw&quot;&gt;宋仲基&lt;/a&gt;和妹妹的旧照&lt;/p&gt;&lt;p&gt;人民网3月18日讯 近日，宋仲基和妹妹的旧照被曝光，因为兄妹相貌相差太大而引发热议，但很多网友表示“有这么帅的‘妹控’哥哥，明明是人生赢家”。&lt;/p&gt;&lt;p&gt;权志龙的姐姐权达美不仅人长得漂亮，也和弟弟一样是一位时尚达人，具有敏锐的时尚感;EXO成员&lt;a  href=&quot;http://piaocanlie.ba.cc&quot;  class=&quot;qkw&quot;&gt;朴灿烈&lt;/a&gt;的姐姐朴宥拉拥有和弟弟一样的好基因，不亚于明星的美貌，并且是韩国某电台的主播;李敏镐的姐姐则非常低调，从较少曝光的几张照片也可以看出，姐姐是典型的韩国女生长相，非常有气质。&lt;/p&gt;&lt;p&gt;和小编一起来看看明星姐弟、兄妹的照片吧，看看到底是神似还是画风不同呢。&lt;/p&gt;&lt;p  class=&quot;text-center&quot;&gt;&lt;img src=&quot;http://p33.qhimg.com/t01f5f3e440e7499fcb.jpg&quot;&gt;&lt;/p&gt;&lt;p class=&quot;img-title&quot;&gt;宋仲基和哥哥、妹妹的童年照片&lt;/p&gt;&lt;p  class=&quot;text-center&quot;&gt;&lt;img src=&quot;http://p35.qhimg.com/t01371503869131cc6a.jpg&quot;&gt;&lt;/p&gt;&lt;p&gt;宋仲基妹妹(右)&lt;a  href=&quot;http://biyezhaopian.ba.cc&quot;  class=&quot;qkw&quot;&gt;毕业照片&lt;/a&gt;&lt;/p&gt;&lt;p  class=&quot;text-center&quot;&gt;&lt;img src=&quot;http://p35.qhimg.com/t01c9e06f250a94a058.jpg&quot;&gt;&lt;/p&gt;&lt;p class=&quot;img-title&quot;&gt;权志龙和姐姐的合照&lt;/p&gt;&lt;p  class=&quot;text-center&quot;&gt;&lt;img src=&quot;http://p31.qhimg.com/t01aa666e8f3c3a69a8.jpg&quot;&gt;&lt;/p&gt;&lt;p&gt;&lt;a  href=&quot;http://quanzhilong.ba.cc&quot;  class=&quot;qkw&quot;&gt;权志龙&lt;/a&gt;和姐姐&lt;/p&gt;&lt;p  class=&quot;text-center&quot;&gt;&lt;img src=&quot;http://p32.qhimg.com/t01948c8f5e0879941b.jpg&quot;&gt;&lt;/p&gt;&lt;p class=&quot;img-title&quot;&gt;权志龙和姐姐合照&lt;/p&gt;&lt;p  class=&quot;text-center&quot;&gt;&lt;img src=&quot;http://p35.qhimg.com/t01c779002bcef0ed6a.jpg&quot;&gt;&lt;/p&gt;&lt;p class=&quot;img-title&quot;&gt;权志龙家庭照&lt;/p&gt;&lt;p  class=&quot;text-center&quot;&gt;&lt;img src=&quot;http://p32.qhimg.com/t01500b9278829b9620.jpg&quot;&gt;&lt;/p&gt;&lt;p class=&quot;img-title&quot;&gt;姐姐和权志龙式同一种style&lt;/p&gt;&lt;p  class=&quot;text-center&quot;&gt;&lt;img src=&quot;http://p35.qhimg.com/t01508fa30b20094829.jpg&quot;&gt;&lt;/p&gt;&lt;p class=&quot;img-title&quot;&gt;朴灿烈和姐姐、妈妈的合照&lt;/p&gt;&lt;p  class=&quot;text-center&quot;&gt;&lt;img src=&quot;http://p31.qhimg.com/t01194437a95a6bcc10.jpg&quot;&gt;&lt;/p&gt;&lt;p class=&quot;img-title&quot;&gt;朴灿烈和姐姐&lt;/p&gt;&lt;p  class=&quot;text-center&quot;&gt;&lt;img src=&quot;http://p33.qhimg.com/t01aa8966f11668e59c.jpg&quot;&gt;&lt;/p&gt;&lt;p class=&quot;img-title&quot;&gt;朴灿烈姐姐&lt;/p&gt;&lt;p  class=&quot;text-center&quot;&gt;&lt;img src=&quot;http://p33.qhimg.com/t01139141f26ab24654.jpg&quot;&gt;&lt;/p&gt;&lt;p  class=&quot;text-center&quot;&gt;&lt;img src=&quot;http://p34.qhimg.com/t015751f2d94202f15a.jpg&quot;&gt;&lt;/p&gt;&lt;p class=&quot;img-title&quot;&gt;秀智和弟弟&lt;/p&gt;&lt;p  class=&quot;text-center&quot;&gt;&lt;img src=&quot;http://p34.qhimg.com/t01bb82ad09bdcd0a93.jpg&quot;&gt;&lt;/p&gt;&lt;p  class=&quot;text-center&quot;&gt;&lt;img src=&quot;http://p34.qhimg.com/t01e3cbea60952109f9.jpg&quot;&gt;&lt;/p&gt;&lt;p class=&quot;img-title&quot;&gt;秀智的弟弟，网友评价“神似卢广仲”&lt;/p&gt;&lt;p  class=&quot;text-center&quot;&gt;&lt;img src=&quot;http://p32.qhimg.com/t0136e14b611642d201.jpg&quot;&gt;&lt;/p&gt;&lt;p class=&quot;img-title&quot;&gt;李敏镐和姐姐的合照&lt;/p&gt;&lt;p  class=&quot;text-center&quot;&gt;&lt;img src=&quot;http://p32.qhimg.com/t01626ada8e3750f4e1.jpg&quot;&gt;&lt;/p&gt;&lt;p class=&quot;img-title&quot;&gt;李敏镐的姐姐(右)&lt;/p&gt;&lt;p  class=&quot;text-center&quot;&gt;&lt;img src=&quot;http://p31.qhimg.com/t01ed8095ab8ae4e212.jpg&quot;&gt;&lt;/p&gt;&lt;p class=&quot;img-title&quot;&gt;李敏镐的姐姐(左)&lt;/p&gt;&lt;p  class=&quot;text-center&quot;&gt;&lt;img src=&quot;http://p35.qhimg.com/t012aaaf012fb8f973d.j