SQL: INSERT INTO `b_article` (`k_pinyinName`,`k_md5name`,`k_sort`,`a_title`,`a_author`,`a_time`,`a_content`,`a_addTime`) VALUES ('wuqilongliushishihunlihuge','846bb8db3f144c3d3076162c945b83ad','2','胡歌因父亲受伤未能参加婚礼 发文祝福刘诗诗吴奇隆','人民网','2016-03-21 14:55:00','&lt;p  class=&quot;text-center&quot;&gt;&lt;img src=&quot;http://p33.qhimg.com/t01ec4061e278c14ee1.jpg&quot;&gt;&lt;/p&gt;&lt;p&gt;21日，&lt;a  href=&quot;http://huge.ba.cc&quot;  class=&quot;qkw&quot;&gt;胡歌&lt;/a&gt;在微博发文汇报父亲的伤情，并表达未能出席&lt;a  href=&quot;http://wuqilong.ba.cc&quot;  class=&quot;qkw&quot;&gt;吴奇隆&lt;/a&gt;、刘诗诗婚礼的遗憾:“老父被摩托车撞倒，左肩断成三截，体内植入钢板和八颗钢钉吃了不少苦头，看着老爷子躺在病床上，还在刷朋友圈点赞诗隆各种婚礼大片，我也是服了!”同时，他还为两位新人送上祝福:“祝福妹妹和妹夫(叫妹夫好爽)，早点让我当舅舅哦!”&lt;/p&gt;&lt;p&gt;对此，有大量网友送上祝福，“老大照顾好爸爸，祝&lt;a  href=&quot;http://laolaoda.ba.cc&quot;  class=&quot;qkw&quot;&gt;老老大&lt;/a&gt;早日康复”、“老爷子好好养伤，我胡好好伺候”、“好好照顾公公”。&lt;/p&gt;','1458611106') [ RunTime:0.0030s ]
