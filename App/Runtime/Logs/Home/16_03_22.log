SQL: INSERT INTO `b_article` (`k_pinyinName`,`k_md5name`,`k_sort`,`a_title`,`a_author`,`a_time`,`a_content`,`a_addTime`) VALUES ('shishangzuipianyiiPhone','6bc5be8922817faa48ce04b40aab84d6','3','苹果发布史上最便宜iPhone!售价比6S便宜近一半，中国首发!买不买?','光明网','2016-03-22 08:41:00','&lt;p class=&quot;header&quot;&gt;又到一年一度&lt;/p&gt;&lt;p&gt;&lt;a  href=&quot;http://pingguo.ba.cc&quot;  class=&quot;qkw&quot;&gt;苹果&lt;/a&gt;春季&lt;/p&gt;&lt;p class=&quot;header&quot;&gt;发(an)布(li)会了!&lt;/p&gt;&lt;p  class=&quot;text-center&quot;&gt;&lt;img src=&quot;http://p35.qhimg.com/t0181d0395fd66e8047.jpg&quot;&gt;&lt;/p&gt;&lt;p&gt;今年的主题为“Let us loop you in”&lt;/p&gt;&lt;p&gt;苹果CEO&lt;a  href=&quot;http://dimukuke.ba.cc&quot;  class=&quot;qkw&quot;&gt;蒂姆·库克&lt;/a&gt;率先登场!&lt;/p&gt;&lt;p class=&quot;header&quot;&gt;乡亲们，想死你们啦!&lt;/p&gt;&lt;p  class=&quot;text-center&quot;&gt;&lt;img src=&quot;http://p31.qhimg.com/t013456dd6170c4595a.jpg&quot;&gt;&lt;/p&gt;&lt;p&gt;库克称，全球已经有10亿苹果设备在使用!&lt;/p&gt;&lt;p class=&quot;header&quot;&gt;你贡献了几台?&lt;/p&gt;&lt;p  class=&quot;text-center&quot;&gt;&lt;img src=&quot;http://p32.qhimg.com/t019364f0d5d58405b0.jpg&quot;&gt;&lt;/p&gt;&lt;p&gt;话不多说，主角直接登场--4寸新&lt;a  href=&quot;http://iPhone.ba.cc&quot;  class=&quot;qkw&quot;&gt;iPhone&lt;/a&gt;&lt;/p&gt;&lt;p class=&quot;header&quot;&gt;iPhone SE&lt;/p&gt;&lt;p  class=&quot;text-center&quot;&gt;&lt;img src=&quot;http://p35.qhimg.com/t01d021284dcc45add3.jpg&quot;&gt;&lt;/p&gt;&lt;p  class=&quot;text-center&quot;&gt;&lt;img src=&quot;http://p32.qhimg.com/t01ea215dd113d3ae9f.jpg&quot;&gt;&lt;/p&gt;&lt;p  class=&quot;text-center&quot;&gt;&lt;img src=&quot;http://p32.qhimg.com/t01c00264f96e64fba7.jpg&quot;&gt;&lt;/p&gt;&lt;p&gt;使用铝合金机身，外观几乎与iPhone5s相同!&lt;/p&gt;&lt;p&gt;性能基本上和iPhone 6s的性能相当，同样搭载1200万像素摄像头，A9处理器，搭载的是最新版的iOS 9.3，苹果称之为“史上最强大的4英寸iPhone”。&lt;/p&gt;&lt;p class=&quot;header&quot;&gt;拥有四种配色。&lt;/p&gt;&lt;p&gt;最重要的价格:谁买谁后悔的16GB型号售价399美元，64GB版本售价为499美元，3月31日出货，首发地区包括中国等五个国家与地区。&lt;/p&gt;&lt;p  class=&quot;text-center&quot;&gt;&lt;img src=&quot;http://p35.qhimg.com/t01687586c89ac49d9f.jpg&quot;&gt;&lt;/p&gt;&lt;p&gt;9.7英寸的&lt;a  href=&quot;http://iPad.ba.cc&quot;  class=&quot;qkw&quot;&gt;iPad&lt;/a&gt; Pro&lt;/p&gt;&lt;p  class=&quot;text-center&quot;&gt;&lt;img src=&quot;http://p31.qhimg.com/t01f7b75293d1eb8095.jpg&quot;&gt;&lt;/p&gt;&lt;p  class=&quot;text-center&quot;&gt;&lt;img src=&quot;http://p32.qhimg.com/t0193f8ac86cd026613.jpg&quot;&gt;&lt;/p&gt;&lt;p  class=&quot;text-center&quot;&gt;&lt;img src=&quot;http://p32.qhimg.com/t013582dc16f800fea9.jpg&quot;&gt;&lt;/p&gt;&lt;p  class=&quot;text-center&quot;&gt;&lt;img src=&quot;http://p33.qhimg.com/t019314893aa2ed574e.jpg&quot;&gt;&lt;/p&gt;&lt;p&gt;iPad Pro也变小到了9.7寸!&lt;/p&gt;&lt;p class=&quot;header&quot;&gt;重量1磅!&lt;/p&gt;&lt;p&gt;摄像头1200万像素，还支持Retina Flash&lt;/p&gt;&lt;p class=&quot;header&quot;&gt;没有3D Touch功能&lt;/p&gt;&lt;p&gt;屏幕可以适应环境色温，比iPad Air 2屏幕亮度高出25%&lt;/p&gt;&lt;p&gt;有一个配套键盘，支持&lt;a  href=&quot;http://Apple.ba.cc&quot;  class=&quot;qkw&quot;&gt;Apple&lt;/a&gt; Pencil&lt;/p&gt;&lt;p&gt;拥有四个扬声器，A9X处理器，足以替代传统笔记本!&lt;/p&gt;&lt;p&gt;售价:32G版本售价599美元，128GB版本售价749美元，256GB版本售价899美元。3月21日可购买。&lt;/p&gt;&lt;p  class=&quot;text-center&quot;&gt;&lt;img src=&quot;http://p31.qhimg.com/t01ba829b85495fd9bc.jpg&quot;&gt;&lt;/p&gt;&lt;p&gt;Apple &lt;a  href=&quot;http://Watch.ba.cc&quot;  class=&quot;qkw&quot;&gt;Watch&lt;/a&gt;推新表带+降价&lt;/p&gt;&lt;p  class=&quot;text-center&quot;&gt;&lt;img src=&quot;http://p35.qhimg.com/t01ea2a8c48561df82f.jpg&quot;&gt;&lt;/p&gt;&lt;p&gt;Apple Watch推出新材质的表带:尼龙。&lt;/p&gt;&lt;p class=&quot;header&quot;&gt;╮(╯_╰)╭&lt;/p&gt;&lt;p&gt;同时，Apple Watch降价，299美元起售。&lt;/p&gt;&lt;p  class=&quot;text-center&quot;&gt;&lt;img src=&quot;http://p34.qhimg.com/t011110f0a8851097a0.jpg&quot;&gt;&lt;/p&gt;','1458611108') [ RunTime:0.0040s ]
