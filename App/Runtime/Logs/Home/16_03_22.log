SQL: INSERT INTO `b_article` (`k_pinyinName`,`k_md5name`,`k_sort`,`a_title`,`a_author`,`a_time`,`a_content`,`a_addTime`) VALUES ('iphonese','2bf96cf1c08afecbb1221e7fbf8a2ed1','10','苹果iPhoneSE今日发布A9芯片超强性能','沃游网','2016-03-22 09:15:27','&lt;p&gt;沃游网3月22日消息，&lt;a  href=&quot;http://pingguogongsi.ba.cc&quot;  class=&quot;qkw&quot;&gt;苹果公司&lt;/a&gt;于北京时间今天凌晨在加州地区的库比蒂诺总部正式召开举行了春季新品发布会，发挥会上正式发布了4英寸的小屏&lt;a  href=&quot;http://iPhone.ba.cc&quot;  class=&quot;qkw&quot;&gt;iPhone&lt;/a&gt;，命名为iPhone SE，苹果公司希望其能接棒iPhone5s，为喜爱小屏手机的用户带来了新的选择，那么就和小编一起看一下其有什么特色吧。&lt;/p&gt;&lt;p  class=&quot;text-center&quot;&gt;&lt;img src=&quot;http://p34.qhimg.com/t01bd474e2f108bc406.jpg&quot;&gt;&lt;/p&gt;&lt;p&gt;苹果公司新的iPhone SE几乎跟与苹果之前的iPhone 5S一模一样，在硬件配置方面，iPhone SE搭载64位&lt;a  href=&quot;http://pingguo.ba.cc&quot;  class=&quot;qkw&quot;&gt;苹果&lt;/a&gt;A9处理芯片，并且辅以M9协处理器，跟iPhone6s一样强大，支持“Hey Siri”唤醒，后置1200万像素摄像头，支持双色温闪光灯，支持全景照片等，支持4K视频拍摄。&lt;/p&gt;&lt;p&gt;在网络方面，支持4G LTE，最高速度达150Mbps，支持VoLTE，另外新机支持苹果Apple Pay支付。售价方面，4英寸的苹果iPhone SE仍然会是&lt;a  href=&quot;http://GB.ba.cc&quot;  class=&quot;qkw&quot;&gt;16GB&lt;/a&gt;起步，16GB售价399美元(约合人民币2586元)，64GB售价499美元(3234元)，将于3月24日的时候接受预定，3月31日正式上市。&lt;/p&gt;','1458611107') [ RunTime:0.0030s ]
