SQL: INSERT INTO `b_article` (`k_pinyinName`,`k_md5name`,`k_sort`,`a_title`,`a_author`,`a_time`,`a_content`,`a_addTime`) VALUES ('dapeng','6d112800958f3d7e668a07aad5ab563c','3','大张伟方回应大鹏 不会原谅当年的污蔑者','网易','2016-03-21 15:41:00','&lt;p&gt;网易娱乐3月21日报道(图文/小易)今日凌晨，大鹏在微博发长文再提及与&lt;a  href=&quot;http://dazhangwei.ba.cc&quot;  class=&quot;qkw&quot;&gt;大张伟&lt;/a&gt;当年的采访事件，他称这7年自己每次见到大张伟都会跟他道歉，但道歉并非是因为所谓自己污蔑了大张伟。中午，大张伟的宣传公司发声明回应了大鹏的“&lt;a  href=&quot;http://beiyuanshuo.ba.cc&quot;  class=&quot;qkw&quot;&gt;被冤说&lt;/a&gt;”，表示无论当年污蔑自己的人是谁，都不会接受他的道歉。&lt;/p&gt;&lt;p  class=&quot;text-center&quot;&gt;&lt;img src=&quot;http://p31.qhimg.com/t01949d08af95301c26.jpg&quot;&gt;&lt;/p&gt;&lt;p&gt;今日凌晨，大鹏在微博发长文再提及与大张伟当年的采访事件，他称这7年自己每次见到大张伟都会跟他道歉，但道歉并非是因为所谓自己污蔑了大张伟。中午，大张伟的宣传公司发声明回应了大鹏的“被冤说”，表示无论当年污蔑自己的人是谁，都不会接受他的道歉。&lt;/p&gt;&lt;p  class=&quot;text-center&quot;&gt;&lt;img src=&quot;http://p31.qhimg.com/t01ff805344f64112fd.jpg&quot;&gt;&lt;/p&gt;&lt;p class=&quot;img-title&quot;&gt;截图&lt;/p&gt;&lt;p  class=&quot;text-center&quot;&gt;&lt;img src=&quot;http://p31.qhimg.com/t016e45d37398c3d3f1.jpg&quot;&gt;&lt;/p&gt;&lt;p  class=&quot;text-center&quot;&gt;&lt;img src=&quot;http://p32.qhimg.com/t01bba8e7d9635e14aa.jpg&quot;&gt;&lt;/p&gt;&lt;p  class=&quot;text-center&quot;&gt;&lt;img src=&quot;http://p35.qhimg.com/t01520625de52f765cd.jpg&quot;&gt;&lt;/p&gt;&lt;p  class=&quot;text-center&quot;&gt;&lt;img src=&quot;http://p33.qhimg.com/t01f46ceab5392ff22e.jpg&quot;&gt;&lt;/p&gt;','1458611105') [ RunTime:0.0030s ]
