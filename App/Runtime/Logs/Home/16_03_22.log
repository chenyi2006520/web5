SQL: INSERT INTO `b_article` (`k_pinyinName`,`k_md5name`,`k_sort`,`a_title`,`a_author`,`a_time`,`a_content`,`a_addTime`) VALUES ('dapeng','661abac8a39d655310aa874f190dc7c9','1','大张伟方发声明回应大鹏不会原谅当年的污蔑者','光明网','2016-03-21 15:10:00','&lt;p  class=&quot;text-center&quot;&gt;&lt;img src=&quot;http://p33.qhimg.com/t01130bf979e6f40a8f.jpg&quot;&gt;&lt;/p&gt;&lt;p&gt;大鹏与&lt;a  href=&quot;http://dazhangwei.ba.cc&quot;  class=&quot;qkw&quot;&gt;大张伟&lt;/a&gt;昔日合影&lt;/p&gt;&lt;p&gt;今日凌晨，大鹏在微博发长文再提及与大张伟当年的采访事件，他称这7年自己每次见到大张伟都会跟他道歉，但道歉并非是因为所谓自己污蔑了大张伟。中午，大张伟的宣传公司发声明回应了大鹏的“被冤说”，表示无论当年污蔑自己的人是谁，都不会接受他的道歉。&lt;/p&gt;&lt;p&gt;大张伟方表示，当年因为和大鹏合作的这段视频流出，导致自己名誉受损，这是极其不负责任和极具毁灭性的，因此无论当年的污蔑者是谁，大张伟都不会接受对方的道歉。&lt;/p&gt;&lt;p  class=&quot;text-center&quot;&gt;&lt;img src=&quot;http://p33.qhimg.com/t01e7e7630c1ce37748.jpg&quot;&gt;&lt;/p&gt;&lt;p class=&quot;img-title&quot;&gt;声明&lt;/p&gt;&lt;p&gt;但大张伟也表示自己很理解大鹏现在的心情，希望两人从此忘记旧事，继续在各自的领域努力下去。&lt;/p&gt;','1458611105') [ RunTime:0.0040s ]
