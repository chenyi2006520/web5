SQL: INSERT INTO `b_article` (`k_pinyinName`,`k_md5name`,`k_sort`,`a_title`,`a_author`,`a_time`,`a_content`,`a_addTime`) VALUES ('songzhixiao','8025fdde633ddd1bda46bd01eba7cddb','4','宋智孝上节目被检查是否整容 获赞天然超童颜美女','光明网','2016-03-21 10:33:00','&lt;p  class=&quot;text-center&quot;&gt;&lt;img src=&quot;http://p34.qhimg.com/t01723005b4e126f4cd.jpg&quot;&gt;&lt;/p&gt;&lt;p&gt;医生赞&lt;a  href=&quot;http://songzhixiao.ba.cc&quot;  class=&quot;qkw&quot;&gt;宋智孝&lt;/a&gt;为超童颜美女&lt;/p&gt;&lt;p&gt;据香港媒体报道，韩国人气综艺节目&lt;a  href=&quot;http://RunningMan.ba.cc&quot;  class=&quot;qkw&quot;&gt;《Running Man》&lt;/a&gt;昨日播出新一集，其中一个环节是解答“宋智孝真人是否真的如此漂亮?”的问题，并请来专家鉴定，邀请成员&lt;a  href=&quot;http://jinzhongguo.ba.cc&quot;  class=&quot;qkw&quot;&gt;金钟国&lt;/a&gt;的哥哥整型医生&lt;a  href=&quot;http://jinzhongming.ba.cc&quot;  class=&quot;qkw&quot;&gt;金钟明&lt;/a&gt;，结果宋智孝被赞是拥有“黄金比例”的美女，因为她的脸型比例正好是1:1:0.8，属超童颜的比例。&lt;/p&gt;&lt;p  class=&quot;text-center&quot;&gt;&lt;img src=&quot;http://p34.qhimg.com/t01b6e91fe95b8cad21.jpg&quot;&gt;&lt;/p&gt;&lt;p&gt;&lt;a  href=&quot;http://chishizhen.ba.cc&quot;  class=&quot;qkw&quot;&gt;池石镇&lt;/a&gt;调皮摆弄宋智孝“小猪鼻”&lt;/p&gt;&lt;p&gt;池石镇更好奇地问“眼睛有没有整过?”，医师表示没有后，池石镇再问智孝“鼻子也没有整过吗?”，智孝更大方地让他摸鼻子证明自己是天然美女，池石镇则调皮地把宋智孝弄成“小猪鼻”，令在场的人不禁爆笑。&lt;/p&gt;','1458611106') [ RunTime:0.0020s ]
