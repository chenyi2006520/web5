SQL: INSERT INTO `b_article` (`k_pinyinName`,`k_md5name`,`k_sort`,`a_title`,`a_author`,`a_time`,`a_content`,`a_addTime`) VALUES ('chuaishuzhizaoyinghuayu','f7ff8fe05aa611d137d9ab2371f7f1c0','2','南京游客被曝 踹树制造樱花雨','东方网','2016-03-21 16:10:37','&lt;p class=&quot;header&quot;&gt;游客爬樱花树&lt;/p&gt;&lt;p class=&quot;header&quot;&gt;游客用脚踹树&lt;/p&gt;&lt;p class=&quot;header&quot;&gt;妈妈带着儿子攀爬城墙&lt;/p&gt;&lt;p&gt;[这样赏花一点都不美]时值春分，多地的樱花都到了盛开的季节。但是在南京，个别游人却为了制造“&lt;a  href=&quot;http://yinghuayu.ba.cc&quot;  class=&quot;qkw&quot;&gt;樱花雨&lt;/a&gt;”，脚踹樱花树、爬树拍照……樱花是用来看的，不是用来蹂躏的!&lt;/p&gt;&lt;p&gt;[风景这么美，你咋这么不美?] 记者采访途中发现，在赏花踏青的人群中竟有这些让人瞠目的举动:妈妈带着儿子攀爬城墙，且不说这城墙的岁数比咱爷爷的爷爷都大，咱该好好保护，就听这孩子 一直喊“害怕”，当妈的还一直托举并“加油”，直到孩子双手滑脱“蹭”了下来喊“疼”，您的心也真大!还有这位老人家，虽说今年是猴年，也不能逮树就上 啊，那树都快折了，旁边的孩子也都看着您呢。&lt;/p&gt;&lt;p  class=&quot;text-center&quot;&gt;&lt;img src=&quot;http://p33.qhimg.com/t01e0844edde62ef41e.jpg&quot;&gt;&lt;/p&gt;&lt;p  class=&quot;text-center&quot;&gt;&lt;img src=&quot;http://p35.qhimg.com/t017189f4ac97ae256f.jpg&quot;&gt;&lt;/p&gt;&lt;p  class=&quot;text-center&quot;&gt;&lt;img src=&quot;http://p33.qhimg.com/t018bc97d568d70a0f5.jpg&quot;&gt;&lt;/p&gt;&lt;p  class=&quot;text-center&quot;&gt;&lt;img src=&quot;http://p34.qhimg.com/t019c10db7f7c5fe3aa.jpg&quot;&gt;&lt;/p&gt;&lt;p  class=&quot;text-center&quot;&gt;&lt;img src=&quot;http://p35.qhimg.com/t01da7e042ecee2068d.jpg&quot;&gt;&lt;/p&gt;','1458611104') [ RunTime:0.0030s ]
