SQL: INSERT INTO `b_article` (`k_pinyinName`,`k_md5name`,`k_sort`,`a_title`,`a_author`,`a_time`,`a_content`,`a_addTime`) VALUES ('pony','b8eb7449984ed6a1d17d09c317968c9f','1','Pony示范惊艳粉紫色','新浪四川','2016-03-18 11:23:04','&lt;p class=&quot;header&quot;&gt;NO.8&lt;/p&gt;&lt;p  class=&quot;text-center&quot;&gt;&lt;img src=&quot;http://p32.qhimg.com/t014656a2914df297c0.jpg&quot;&gt;&lt;/p&gt;&lt;p class=&quot;img-title&quot;&gt;樱花粉+熏衣草紫色&lt;/p&gt;&lt;p class=&quot;header&quot;&gt;发色:樱花粉+熏衣草紫色&lt;/p&gt;&lt;p&gt;代表博主:&lt;a  href=&quot;http://Pony.ba.cc&quot;  class=&quot;qkw&quot;&gt;Pony&lt;/a&gt;&lt;/p&gt;&lt;p&gt;樱花粉+熏衣草紫色，美出&lt;a  href=&quot;http://tianjixian.ba.cc&quot;  class=&quot;qkw&quot;&gt;天际线&lt;/a&gt;了~虽然不那么实用，但它实在太迷人了，为什么不能尝试看看呢?&lt;/p&gt;&lt;p  class=&quot;text-center&quot;&gt;&lt;img src=&quot;http://p31.qhimg.com/t014dc122391071f9ec.jpg&quot;&gt;&lt;/p&gt;&lt;p class=&quot;img-title&quot;&gt;pony2&lt;/p&gt;&lt;p&gt;两个撞色发色配上pony大神的惊艳妆容，不要太时髦好嘛~&lt;/p&gt;&lt;p class=&quot;header&quot;&gt;NO.9&lt;/p&gt;&lt;p  class=&quot;text-center&quot;&gt;&lt;img src=&quot;http://p35.qhimg.com/t01530905eba73417ba.jpg&quot;&gt;&lt;/p&gt;&lt;p class=&quot;img-title&quot;&gt;巧克力&lt;/p&gt;&lt;p class=&quot;header&quot;&gt;发色:巧克力色&lt;/p&gt;&lt;p&gt;代表女星:&lt;a  href=&quot;http://Lily.ba.cc&quot;  class=&quot;qkw&quot;&gt;Lily&lt;/a&gt; Collins&lt;/p&gt;&lt;p&gt;巧克力色不是新鲜的发色了，却依然颇受欢迎。因为它兼具奢华的质感和百搭的气质!Lily &lt;a  href=&quot;http://Collins.ba.cc&quot;  class=&quot;qkw&quot;&gt;Collins&lt;/a&gt;这款巧克力色蛋卷bobo头，即复古摩登又灵动俏皮，十分养眼。&lt;/p&gt;&lt;p  class=&quot;text-center&quot;&gt;&lt;img src=&quot;http://p32.qhimg.com/t011f496d89e64ffd48.jpg&quot;&gt;&lt;/p&gt;&lt;p&gt;&lt;a  href=&quot;http://tiffany.ba.cc&quot;  class=&quot;qkw&quot;&gt;tiffany&lt;/a&gt;&lt;/p&gt;&lt;p&gt;少女时代的tiffany也曾在大片中演绎过巧克力发色，充满了怀旧感，女人味秒涨!&lt;/p&gt;&lt;p class=&quot;header&quot;&gt;NO.10&lt;/p&gt;&lt;p  class=&quot;text-center&quot;&gt;&lt;img src=&quot;http://p32.qhimg.com/t0131b0ab0d797a3288.jpg&quot;&gt;&lt;/p&gt;&lt;p class=&quot;img-title&quot;&gt;波尔多红&lt;/p&gt;&lt;p class=&quot;header&quot;&gt;发色:波尔多红色&lt;/p&gt;&lt;p&gt;代表女星:&lt;a  href=&quot;http://Emily.ba.cc&quot;  class=&quot;qkw&quot;&gt;Emily&lt;/a&gt; Ratajkowski&lt;/p&gt;&lt;p&gt;性感的Emily &lt;a  href=&quot;http://Ratajkowski.ba.cc&quot;  class=&quot;qkw&quot;&gt;Ratajkowski&lt;/a&gt;选择了波尔多红色，神秘又性感的魅力从发丝开始绽放~&lt;/p&gt;&lt;p  class=&quot;text-center&quot;&gt;&lt;img src=&quot;http://p32.qhimg.com/t012bc7188986c2321c.jpg&quot;&gt;&lt;/p&gt;&lt;p class=&quot;img-title&quot;&gt;Emily Ratajkowski&lt;/p&gt;&lt;p&gt;这种发色搭配上&lt;a  href=&quot;http://mitang.ba.cc&quot;  class=&quot;qkw&quot;&gt;蜜糖&lt;/a&gt;肌更能衬托出那种浓浓的魅惑风情，衬出满满的女人味。&lt;/p&gt;&lt;p class=&quot;header&quot;&gt;NO.11&lt;/p&gt;&lt;p  class=&quot;text-center&quot;&gt;&lt;img src=&quot;http://p34.qhimg.com/t016a460b16be440eee.jpg&quot;&gt;&lt;/p&gt;&lt;p class=&quot;img-title&quot;&gt;自然黑&lt;/p&gt;&lt;p class=&quot;header&quot;&gt;发色:自然黑色&lt;/p&gt;&lt;p&gt;代表女星:&lt;a  href=&quot;http://fanbingbing.ba.cc&quot;  class=&quot;qkw&quot;&gt;范冰冰&lt;/a&gt;&lt;/p&gt;&lt;p&gt;遭遇过染发的头发最终都会变成枯萎的黄色，也许这个时候有人会认为回归到黑长直才是正确的选择。自然黑永远是帮你拉回到女神高位的一条捷径，比如&lt;a  href=&quot;http://fanye.ba.cc&quot;  class=&quot;qkw&quot;&gt;范爷&lt;/a&gt;就是万年黑发的最佳代言人。&lt;/p&gt;&lt;p  class=&quot;text-center&quot;&gt;&lt;img src=&quot;http://p32.qhimg.com/t01f3549e2ab095f063.jpg&quot;&gt;&lt;/p&gt;&lt;p class=&quot;img-title&quot;&gt;范冰冰&lt;/p&gt;&lt;p&gt;别忘了，黑长直才是女神标配!&lt;/p&gt;','1458611107') [ RunTime:0.0040s ]
