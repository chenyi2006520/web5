SQL: INSERT INTO `b_article` (`k_pinyinName`,`k_md5name`,`k_sort`,`a_title`,`a_author`,`a_time`,`a_content`,`a_addTime`) VALUES ('hugebalidao','b42ca374b6cdce825d16285d70d447bc','1','网曝胡歌父亲腿伤缺席刘诗诗吴奇隆巴厘岛大婚','','2016-03-21 14:10:46','&lt;p&gt;据台湾媒体报道，&lt;a  href=&quot;http://wuqilong.ba.cc&quot;  class=&quot;qkw&quot;&gt;吴奇隆&lt;/a&gt;与刘诗诗明天(20日)将在&lt;a  href=&quot;http://balidao.ba.cc&quot;  class=&quot;qkw&quot;&gt;巴厘岛&lt;/a&gt;举办婚礼，宾客众星云集，伴郎是小虎队的苏有朋和陈志朋，对岸当红男神霍建华、&lt;a  href=&quot;http://huge.ba.cc&quot;  class=&quot;qkw&quot;&gt;胡歌&lt;/a&gt;等人也都会出席，高颜值婚礼让外界引颈期盼，今(19日)却传出胡歌将缺席。&lt;/p&gt;&lt;p  class=&quot;text-center&quot;&gt;&lt;img src=&quot;http://p35.qhimg.com/t01d0d1c3cf6bb08e7c.jpg&quot;&gt;&lt;/p&gt;&lt;p class=&quot;img-title&quot;&gt;胡歌将缺席隆诗婚礼&lt;/p&gt;&lt;p&gt;网曝出胡歌父亲骨折送医，胡歌在得知消息后奔往上海的医院探视，据媒体报道，胡歌可能因此不前往巴厘岛参加隆诗婚礼。&lt;/p&gt;','1458611105') [ RunTime:0.0040s ]
